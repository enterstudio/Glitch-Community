
article.projects(style="background-color: #{@background}")
  header.category
    a.category-name(href=@url)
      h2= @name
        span.arrow →
    span.category-image-container
      a.category-image(href=@url)
        img(src=@img)
    p.category-description= @description

  ul.projects-container
    - console.log @projects
    - @projects.forEach (project) ->

      - console.log "❤️", project

      = project

